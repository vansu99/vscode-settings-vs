{
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "SF Mono, Fire Code, Arial, sans-serif",
  "editor.tabSize": 2,
  "editor.letterSpacing": 0.4,
  "editor.lineHeight": 32,
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "explorer.confirmDelete": false,
  "files.autoSave": "afterDelay",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.highlightModifiedTabs": true,
  "eslint.alwaysShowStatus": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.triggerExpansionOnTab": true,
  "workbench.fontAliasing": "auto",
  "editor.cursorBlinking": "expand",
  "git.autofetch": true,
  "git.confirmSync": false,
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "extensions.autoUpdate": true,
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.fontFamily": "monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.lineHeight": 1.4,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontWeightBold": "500",
  "prettier.semi": true,
  "prettier.printWidth": 90,
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "eslint.format.enable": true,
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "workbench.startupEditor": "none",
  "editor.wordWrapColumn": 100,
  "color-highlight.markerType": "underline",
  "liveServer.settings.donotShowInfoMsg": true,
  "workbench.colorCustomizations": {
    "editorCursor.background": "#cc4b17",
    "editorCursor.foreground": "#c79d13"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.inlineSuggest.enabled": true,
  "liveServer.settings.donotVerifyTags": true,
  "[php]": {
    "editor.defaultFormatter": "Sophisticode.php-formatter"
  },
  "javascript.inlayHints.parameterNames.enabled": "all",
  "editor.inlayHints.enabled": "on",
  "security.workspace.trust.untrustedFiles": "open",
  "tabnine.experimentalAutoImports": true,
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "workbench.colorTheme": "Bearded Theme Arc Blueberry",
  "editor.fontSize": 13
}
