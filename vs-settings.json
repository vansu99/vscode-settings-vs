{
  "editor.fontFamily": "MonoLisa, SF Mono,Cascadia Code, JetBrains Mono, sans-serif",
  "editor.fontSize": 15,
  "editor.fontWeight": 400,
  "editor.fontLigatures": true,
  "editor.detectIndentation": true,
  "editor.lightbulb.enabled": "off",
  "editor.tabSize": 2,
  "editor.lineHeight": 2,
  "editor.smoothScrolling": true,
  "editor.formatOnSave": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.suggestSelection": "recentlyUsed"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit"
  },
  "tailwindCSS.experimental.classRegex": ["[a-zA-Z]*class[a-zA-Z]*='([^']+)'"],
  "workbench.editor.empty.hint": "hidden",
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.fontFamily": "SF Mono",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.lineHeight": 1.4,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontWeightBold": "500",
  "workbench.colorCustomizations": {
    "editorCursor.background": "#F6E1C3",
    "editorCursor.foreground": "#FFC94A"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "workbench.startupEditor": "none",
  "screencastMode.fontSize": 58,
  "todohighlight.isEnable": true,
  "workbench.tree.enableStickyScroll": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.stickyScroll.maxLineCount": 1,
  "editor.stickyScroll.enabled": false,
  "scm.showOutgoingChanges": "never",
  "prettier.printWidth": 100,
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "One Dark italic",
        "scope": [
          "comment",
          "entity.other.attribute-name",
          "keyword",
          "markup.underline.link",
          "storage.modifier",
          "storage.type",
          "string.url",
          "variable.language.super",
          "variable.language.this"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "One Dark italic reset",
        "scope": [
          "keyword.operator",
          "keyword.other.type",
          "storage.modifier.import",
          "storage.modifier.package",
          "storage.type.built-in",
          "storage.type.function.arrow",
          "storage.type.generic",
          "storage.type.java",
          "storage.type.primitive"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "workbench.colorTheme": "Atom One Dark",
  "workbench.iconTheme": "material-icon-theme"
}
