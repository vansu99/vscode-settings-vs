{
  "editor.fontFamily": "Cascadia Code, SF Mono, 'Dank Mono', sans-serif",
  "editor.fontSize": 15,
  "editor.fontWeight": "370",
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
  "editor.detectIndentation": true,
  "editor.lightbulb.enabled": false,
  "editor.letterSpacing": 0.95,
  "editor.tabSize": 2,
  "editor.lineHeight": 2,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "explorer.confirmDelete": false,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.suggestSelection": "recentlyUsed"
  },
  "javascript.validate.enable": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.suggestSelection": "recentlyUsed"
  },
  "workbench.editor.highlightModifiedTabs": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.triggerExpansionOnTab": true,
  "emmet.showSuggestionsAsSnippets": true,
  "editor.cursorBlinking": "expand",
  "git.autofetch": true,
  "git.confirmSync": false,
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "extensions.autoUpdate": true,
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.fontFamily": "SF Mono",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.lineHeight": 1.4,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontWeightBold": "500",
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "workbench.startupEditor": "none",
  "editor.wordWrapColumn": 100,
  "color-highlight.markerType": "underline",
  "workbench.colorCustomizations": {
    "editorCursor.background": "#F6E1C3",
    "editorCursor.foreground": "#E9A178"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.inlineSuggest.enabled": true,
  "[php]": {
    "editor.defaultFormatter": "Sophisticode.php-formatter"
  },
  "javascript.inlayHints.parameterNames.enabled": "all",
  "editor.codeLens": false,
  "security.workspace.trust.untrustedFiles": "open",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "javascript.referencesCodeLens.enabled": false,
  "typescript.referencesCodeLens.enabled": false,
  "workbench.iconTheme": "material-icon-theme",
  "editor.inlayHints.fontFamily": "Fira Code",
  "editor.inlayHints.fontSize": 12,
  "editor.inlayHints.padding": true,
  "editor.inlayHints.enabled": "off",
  "workbench.colorTheme": "Dracula",
  "tailwindCSS.experimental.classRegex": ["[a-zA-Z]*class[a-zA-Z]*='([^']+)'"]
}
